n{\r\n            refreetButton:{\r\n                text: 'Refreet'\r\n            },\r\n            showRefreeters: false,\r\n            Refreeted: false,\r\n            open: false,\r\n            comments: \"\",\r\n            \r\n       }\r\n   },\r\n    created() {\r\n        \r\n  },\r\n    methods:{\r\n    AddComment(){\r\n        this.open = true;\r\n        \r\n    },\r\n    Refreet(){\r\n        axios\r\n            .post(\"/api/refreets/post/\" + this.freet.id, {\r\n                comments: this.comments\r\n                })\r\n            .then((response) => {\r\n                this.open=false;\r\n                eventBus.$emit(\"refreet-success\", {\r\n                    data: response.data,\r\n                    });\r\n            this.comments=''\r\n               \r\n            })\r\n            .catch((error) => {\r\n                if (error.response && error.response.status != 200){\r\n                alert(error.response.data.error)\r\n                }\r\n            })\r\n        },\r\n  \r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n.blue-color{\r\n    color:#3CA3D8;\r\n}\r\n</style>"],"sourceRoot":"src/components/Freets"}]}