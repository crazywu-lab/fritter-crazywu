;AAyBA,EAAA,OAzBA,qBAyBA;AAAA;;AACA,SAAA,QAAA;AACA,IAAA,QAAA,CAAA,GAAA,CAAA,CAAA,sBAAA,EACA,oBADA,EACA,wBADA,CAAA,EACA,YAAA;AACA,MAAA,KAAA,CAAA,QAAA;AACA,KAHA;AAIA,GA/BA;AAiCA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,gBAAA,KAAA,OAAA,CAAA,OAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,mBAAA,EAAA;AACA,UAAA,IAAA,EAAA,QAAA,CAAA;AADA,SAAA;;AAGA,YAAA,QAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,QAAA,CAAA,IAAA;AACA;AAEA,OATA,EAUA,KAVA,CAUA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,IAAA,KAAA,CAAA,QAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,IAAA;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA;AACA,OAfA;AAgBA,KAlBA;AAoBA,IAAA,aApBA,2BAoBA;AAAA;;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,0BAAA,KAAA,OAAA,CAAA,EAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,wBAAA,EAAA;AACA,UAAA,IAAA,EAAA,QAAA,CAAA;AADA,SAAA;AAIA,OANA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,OAAA;;AACA,YAAA,KAAA,CAAA,QAAA,IAAA,KAAA,CAAA,QAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KAAA,CAAA;AACA;AACA,OAZA;AAcA;AAnCA;AAjCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"refreet\">\r\n    <button  \r\n      class=\"delete-button\" \r\n      v-show=\"userName===refreet.refreeter && !upvoted\"\r\n      v-on:click=\"DeleteRefreet()\"><i class=\"fa fa-trash\"></i>\r\n    </button>\r\n    <h5> <span>@{{ refreet.id }} {{ refreet.timestamp.slice(0,10) }} </span></h5>\r\n    <h4> {{ refreet.refreeter }}:<span> {{refreet.comments}}</span></h4> \r\n    <div class=\"original-freet\" v-if='!error'>\r\n    <p>@{{ originalFreet.id }} {{ originalFreet.timestamp.slice(0,10)}}</p>\r\n    <span>{{ originalFreet.author }}</span>: {{originalFreet.content }}</div>\r\n    \r\n    <section class=\"original-freet\" v-else>{{ originalFreet }}</section>\r\n    <p><UpvoteButton :freet = refreet :userName = userName /> </p>  \r\n\r\n    <div class=\"buttons\">   \r\n   \r\n\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport { eventBus } from \"../../main\";\r\nimport axios from 'axios';\r\nimport UpvoteButton from \"@/components/Freets/UpvoteButton.vue\";\r\n\r\nexport default {\r\n  name: \"Refreet\",\r\n\r\n  props: {\r\n    refreet: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n    upvoted: {\r\n      type: Object,\r\n      required: false,\r\n    },\r\n  },\r\n  components:{\r\n    UpvoteButton, \r\n  },\r\n    data() {\r\n        return{\r\n        userName: this.$cookie.get('fritter-auth'),\r\n        originalFreet: '',\r\n        error: false,\r\n        }\r\n\r\n\r\n  },\r\n  created() {\r\n    this.GetFreet() \r\n    eventBus.$on([ \"delete-freet-success\",\r\n                    \"edit-freet-success\",\"delete-refreet-success\"], ()=>{\r\n        this.GetFreet();\r\n    });\r\n  },\r\n\r\n  methods:{\r\n      GetFreet() {\r\n      axios.get('api/freets/'+this.refreet.freetId)\r\n        .then((response) => {\r\n            eventBus.$emit(\"get-freet-success\",{\r\n            data: response.data,\r\n            });\r\n            if(response.data){\r\n                this.originalFreet = response.data;\r\n            }\r\n\r\n        })\r\n        .catch((error) => {\r\n            if (error.response && error.response.status != 200){\r\n                this.error=true;\r\n                this.originalFreet = error.response.data.error\r\n            }\r\n          })\r\n  },\r\n\r\n      DeleteRefreet(){\r\n      axios.delete(\"/api/refreets/delete/\" + this.refreet.id)\r\n       .then((response) => {\r\n            eventBus.$emit(\"delete-refreet-success\",{\r\n              data: response.data,\r\n            });\r\n         \r\n          })\r\n       .catch((error) => {\r\n           console.log(this.refreet)\r\n            if (error.response && error.response.status != 200){\r\n              alert(error.response.data.error)\r\n            }\r\n          })\r\n    \r\n    },\r\n  \r\n    }\r\n\r\n}\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.refreet {\r\n  padding: 10px;\r\n  width: calc(100% - 60px);\r\n  margin: 15px;\r\n  margin-bottom: 18px;\r\n  background-color: white;\r\n  border-radius: 10px;\r\n  box-shadow: 1px 1px 6px 0 rgba(31,38,135,0.3);\r\n}\r\n.refreet > p{  \r\n  overflow-wrap: break-word;\r\n}\r\n.refreet > h4  {\r\n  font-size:11.0pt;\r\n}\r\n.refreet > h4 > span {\r\n  font-weight: 100;\r\n}\r\n.refreet > h5 > span {\r\n  font-weight: 100;\r\n  display: flex;\r\n  font-size:10.0pt;\r\n  color: #104763;\r\n}\r\n.original-freet {\r\n  overflow-wrap: break-word; \r\n  padding: 10px;\r\n  width: 250px auto ;\r\n  margin: 15px;\r\n  margin-bottom: 18px;\r\n  border: 3px solid #ddd;\r\n  background-color: rgba(212, 233, 245, 0.432);\r\n  font-size:11.0pt;\r\n  \r\n}\r\n.original-freet >span {\r\n  font-weight:bold;\r\n  font-size:11.0pt; \r\n}\r\n.original-freet >p {\r\n  font-weight: 100;\r\n  display: flex;\r\n  font-size:10.0pt;\r\n  color: #104763;\r\n}\r\n\r\n\r\n\r\n.buttons{\r\n  display:flex; \r\n  justify-content:flex-end;\r\n  \r\n}\r\n.delete-button{\r\n  float: right;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Freets"}]}